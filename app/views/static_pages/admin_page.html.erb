<div class="maincontent">
<h2>Utenti</h2>

 <%= render "user" %><br/>

<h2>Modulo 1 da approvare </h2>
<table class="table1">
    <tr>
      <th>Nome progetto</th>
      <th>Area</th>
      <th>Creato il:</th>    
      <th>Ultima Modifica il:</th>
      <th>Approvato</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  

  
<% @modulo1s.each do |modulo1| %>
<% if modulo1.Approvato == false %>
      <tr>
        <td><%= modulo1.nome_progetto %>&nbsp;&nbsp;&nbsp;&nbsp;</td>
        <td><%= modulo1.macrocat_sett_scient %></td>
        <td><%= l(modulo1.created_at, format: "%e %B %Y %H:%M:%S") %>&nbsp;&nbsp;&nbsp;&nbsp;</td>
        <td><%= l(modulo1.updated_at, format: "%e %B %Y %H:%M:%S") %>&nbsp;&nbsp;&nbsp;&nbsp;</td>
        <td><%= modulo1.Approvato%></td>
        <td><%= link_to 'Mostra', [modulo1.user, modulo1] %>&nbsp;&nbsp;</td>
        <td><%= link_to 'Modifica', edit_user_modulo1_path(modulo1.user, modulo1)%>&nbsp;&nbsp;</td>
        <td><%= link_to 'Elimina', [modulo1.user, modulo1], :confirm => 'Sei sicuro?', :method => :delete %></td>
      </tr>
    <% end %>
      <% end %>

</table>




</div>