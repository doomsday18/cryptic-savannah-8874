<div class="maincontent">
<hr>
<p class="ricercaavanzata"><%= link_to "Cerca tra tutti i progetti esistenti", new_modulo1search_path %></p>
<hr>
<p class="pulsanteblu"><%= link_to "Consulta tutti i Progetti", menumetadati_path %></p>
<hr>
<h3>Lista delle proposte dell'utente: </h3>
<p><%=current_user.email%></p><br/>
<%= will_paginate @modulo1s, :previous_label => 'Prec', :next_label => 'Succ' %>
<table class="table1">
    <tr>
      <th>Nome progetto</th>
      <th>Area</th>
      <th>Creato il:</th>    
      <th>Ultima Modifica il:</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  

  
<% @modulo1s.each do |modulo1| %>
      <tr>
        <td><%= modulo1.nome_progetto %>&nbsp;&nbsp;&nbsp;&nbsp;</td>
        <td><%= modulo1.macrocat_sett_scient %></td>
        <td><%= l(modulo1.created_at, format: "%e %B %Y %H:%M:%S") %>&nbsp;&nbsp;&nbsp;&nbsp;</td>
        <td><%= l(modulo1.updated_at, format: "%e %B %Y %H:%M:%S") %>&nbsp;&nbsp;&nbsp;&nbsp;</td>

        
        <td><%= link_to 'Mostra', [modulo1.user, modulo1] %>&nbsp;&nbsp;</td>
        <td><%= link_to 'Modifica', edit_user_modulo1_path(modulo1.user, modulo1),'data-no-turbolink' => true %>&nbsp;&nbsp;</td>
        <td><%= link_to 'Elimina', [modulo1.user, modulo1], :confirm => 'Sei sicuro?', :method => :delete %></td>
      </tr>
    <% end %>

</table>
<%= will_paginate @modulo1s, :previous_label => 'Prec', :next_label => 'Succ' %>
<br />
<hr>
<p class="pulsanteverde"><%= link_to 'Proponi un nuovo Modulo 1', new_user_modulo1_path %> </p>
<p class="pulsanteblu"><%= link_to 'Torna alla pagina Utente', user_pages_path %></p>
<hr>
<br/><br/><br/><br/><br/>
<%= render 'layouts/footer' %>
</div>

